{
  "openapi": "3.1.0",
  "info": {
    "title": "form_aggregation_sdk API",
    "description": "form_aggregation_sdk API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://form_aggregation_sdk"
    }
  ],
  "tags": [
    {
      "name": "线索管理",
      "description": "用于测试线索管理的接口，包含线索的生成、推送等功能"
    }
  ],
  "paths": {
    "/api/agg/update": {
      "put": {
        "summary": "更新线索",
        "description": "更新指定的线索信息",
        "operationId": "updateClue",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Clue"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ClueCollectionResponseData"
                }
              }
            }
          }
        }
      }
    },
    "/api/agg/random-push": {
      "post": {
        "summary": "生成随机线索并推送",
        "description": "根据种子库生成随机的线索信息，并推送到指定的推送地址",
        "operationId": "randomPush",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ClueCollectionResponseData"
                }
              }
            }
          }
        }
      }
    },
    "/api/agg/bulk-push": {
      "post": {
        "summary": "批量生成随机线索并推送",
        "description": "根据种子库生成指定数量的随机的线索信息，并推送到指定的推送地址",
        "operationId": "bulkRandomPush",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/BulkRandomPush"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/BulkPushResult"
                }
              }
            }
          }
        }
      }
    },
    "/api/agg/push": {
      "post": {
        "summary": "推送线索",
        "description": "推送指定的线索信息到指定的推送地址",
        "operationId": "push",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Clue"
              }
            }
          },
          "required": true
        },
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/ClueCollectionResponseData"
                }
              }
            }
          }
        }
      }
    },
    "/api/agg/ping": {
      "get": {
        "summary": "测试服务是否正常",
        "description": "用于测试服务是否正常",
        "operationId": "ping",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/api/agg/random": {
      "get": {
        "summary": "生成随机线索",
        "description": "根据种子库生成随机的线索信息",
        "operationId": "random",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "*/*": {
                "schema": {
                  "$ref": "#/components/schemas/Clue"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Clue": {
        "type": "object",
        "properties": {
          "clueId": {
            "type": "string",
            "title": "线索id",
            "description": "线索id"
          },
          "realTimePush": {
            "type": "boolean",
            "title": "是否实时推送",
            "description": "是否实时推送，true表示实时推送，false表示非实时推送"
          },
          "specifiedAppCode": {
            "type": "string",
            "title": "指定推送的appCode",
            "description": "指定推送的appCode"
          },
          "customerName": {
            "type": "string",
            "title": "客户姓名",
            "description": "客户姓名"
          },
          "gender": {
            "type": "integer",
            "format": "int32",
            "title": "性别",
            "description": "性别，1表示男，2表示女"
          },
          "phone": {
            "type": "string",
            "title": "手机号",
            "description": "手机号"
          },
          "residentPlace": {
            "type": "string",
            "title": "居住地",
            "description": "居住地"
          },
          "provinceCode": {
            "type": "string",
            "title": "省份编码",
            "description": "省份编码"
          },
          "cityCode": {
            "type": "string",
            "title": "城市编码",
            "description": "城市编码"
          },
          "areaCode": {
            "type": "string",
            "title": "区县编码",
            "description": "区县编码"
          },
          "birthYear": {
            "type": "integer",
            "format": "int32",
            "title": "出生年",
            "description": "出生年"
          },
          "birthMonth": {
            "type": "integer",
            "format": "int32",
            "title": "出生月",
            "description": "出生月"
          },
          "birthDay": {
            "type": "integer",
            "format": "int32",
            "title": "出生日",
            "description": "出生日"
          },
          "age": {
            "type": "integer",
            "format": "int32",
            "title": "年龄",
            "description": "年龄"
          },
          "ageRange": {
            "type": "string",
            "title": "年龄段",
            "description": "年龄段"
          },
          "previouslyInsured": {
            "type": "string",
            "title": "是否有保险",
            "description": "是否有保险"
          },
          "consultingNeeds": {
            "type": "string",
            "title": "咨询需求",
            "description": "咨询需求"
          },
          "insuranceBudget": {
            "type": "string",
            "title": "保险预算",
            "description": "保险预算"
          },
          "personalAnnualIncome": {
            "type": "string",
            "title": "个人年收入",
            "description": "个人年收入"
          },
          "familyAnnualIncome": {
            "type": "string",
            "title": "家庭年收入",
            "description": "家庭年收入"
          },
          "authorizedOrganization": {
            "type": "string",
            "title": "授权机构",
            "description": "授权机构"
          },
          "channelCode": {
            "type": "string",
            "title": "渠道编码",
            "description": "渠道编码"
          },
          "subChannelCode": {
            "type": "string",
            "title": "子渠道编码",
            "description": "子渠道编码"
          },
          "clueGrade": {
            "type": "string",
            "title": "线索等级",
            "description": "线索等级"
          },
          "idCardNo": {
            "type": "string",
            "title": "身份证号",
            "description": "身份证号"
          },
          "productCode": {
            "type": "string",
            "title": "产品编码",
            "description": "产品编码"
          },
          "productVersion": {
            "type": "string",
            "title": "产品版本",
            "description": "产品版本"
          },
          "tags": {
            "type": "array",
            "items": {
              "type": "string",
              "title": "标签",
              "description": "标签"
            }
          },
          "useType": {
            "type": "integer",
            "format": "int32",
            "title": "使用类型",
            "description": "使用类型，1 正式 2 测试"
          },
          "ext": {
            "type": "object"
          }
        }
      },
      "ClueCollectionResponseData": {
        "type": "object",
        "properties": {
          "requestId": {
            "type": "string"
          },
          "clueId": {
            "type": "string"
          }
        }
      },
      "BulkRandomPush": {
        "type": "object",
        "properties": {
          "count": {
            "type": "integer",
            "format": "int32"
          },
          "ignoreError": {
            "type": "boolean"
          }
        }
      },
      "BulkPushResult": {
        "type": "object",
        "properties": {
          "successCount": {
            "type": "integer",
            "format": "int32"
          },
          "failCount": {
            "type": "integer",
            "format": "int32"
          }
        }
      }
    }
  }
}